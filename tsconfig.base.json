{
  "compilerOptions": {
    "target": "ES2018",
    "lib": ["DOM", "ESNext", "DOM.Iterable"],
    "jsx": "react",
    "moduleResolution": "Node",
    "sourceMap": true,
    "declarationMap": true,
    "declaration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "importHelpers": true
  }
}
